{
    "name": "source-scraper-test-utils",
    "version": "1.1.1",
    "description": "Test Utils for the SourceScraper-Project",
    "author": "OpenByte <development.openbyte@gmail.com>",
    "main": "dist/index.js",
    "types": "types/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/OpenByteDev/SourceScraper.git"
    },
    "dependencies": {
        "chai": "^4.2.0",
        "mocha": "^5.2.0",
        "source-scraper-core": "^1.1.1"
    },
    "devDependencies": {
        "@types/chai": "^4.1.6",
        "@types/mocha": "^5.2.5",
        "@types/node": "^10.12.0",
        "npm-run-all": "^4.1.3",
        "ts-typie": "^1.1.1",
        "tslint": "^5.11.0",
        "tslint-eslint-rules": "^5.4.0",
        "typedoc": "^0.13.0",
        "typescript": "^3.1.3"
    },
    "scripts": {
        "ci-test": "npm run test",
        "test": "npm-run-all lint build",
        "lint": "tslint -p .",
        "lint-fix": "tslint -p . --fix",
        "build": "tsc",
