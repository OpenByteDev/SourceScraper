{
    "name": "mp4upload-scraper",
    "version": "0.2.10",
    "description": "A scraper for MP4Upload",
    "author": "OpenByte <development.openbyte@gmail.com>",
    "main": "dist/index.js",
    "types": "types/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/OpenByteDev/SourceScraper.git"
    },
    "dependencies": {
        "source-scraper-core": "^1.0.0",
        "source-scraper-jwplayer-runner": "^1.0.0"
    },
    "devDependencies": {
        "@types/mocha": "^5.2.5",
        "@types/node": "^10.7.1",
        "@types/typescript": "^2.0.0",
        "mocha": "^5.2.0",
        "npm-run-all": "^4.1.3",
        "ts-typie": "^1.1.1",
        "source-scraper-test-utils": "^1.0.0",
        "ts-node": "^7.0.1",
        "tslint": "^5.11.0",
        "tslint-eslint-rules": "^5.4.0",
        "typedoc": "^0.12.0",
        "typescript": "^3.0.1"
    },
    "scripts": {
        "ci-test": "npm-run-all lint build test",
        "test": "mocha test/*.spec.ts",
