{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:latest"
    ],
    "jsRules": {},
    "rules": {
        "linebreak-style": [true, "CRLF"],
        "trailing-comma": false,
        "object-literal-sort-keys": false,
        "no-eval": false,
        "arrow-parens": false,
        "no-conditional-assignment": false,
        "variable-name": false,
        "max-line-length": [true, 120],
        "interface-name": false,
        "curly": false,
        "no-console": false,
        "quotemark": [true, "single"]
    },
    "rulesDirectory": []
}